{
    "manifest_version": 3,
    "name": "Tabularize",
    "description": "Groups your open browser tabs automatically and applies smart labels",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "images/logo.png"
    },
    "icons": {
        "128": "images/logo.png"
    },
    "permissions": [
        "tabs",
        "tabGroups"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}