{
    "manifest_version": 3,
    "name": "Tabularize",
    "description": "Groups your open browser tabs automatically and applies smart labels",
    "version": "0.1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "/icons/logo.png"
    },
    "icons": {
        "128": "/icons/logo.png"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "serviceWorker.js"
    }
}